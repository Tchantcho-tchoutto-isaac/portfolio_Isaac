    <section class="contact" id="contact">
      <h2 class="heading">contactez-<span>moi !</span></h2>
      <form action="#">
        <div class="input-box">
          <div class="input-field">
            <input type="text" placeholder="Nom et prénom" required />
            <span class="focus"></span>
          </div>
          <div class="input-field">
            <input type="text" placeholder="Adresse e-mail" required />
            <span class="focus"></span>
          </div>
        </div>

        <div class="input-box">
          <div class="input-field">
            <input type="Number" placeholder="Numéro de portable" required />
            <span class="focus"></span>
          </div>
          <div class="input-field">
            <input type="text" placeholder="Sujet du courriel" required />
            <span class="focus"></span>
          </div>
        </div>
        <div class="textarea-field">
          <textarea
            name=""
            id=""
            cols="30"
            rows="10"
            placeholder="Votre message"
            required
